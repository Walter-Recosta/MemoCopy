{
  "manifest_version": 3,
  "name": "Almacenamiento Personal",
  "version": "1.0",
  "description": "Extensión para guardar documentos, códigos y más.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
